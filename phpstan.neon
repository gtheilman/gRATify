includes:
  - ./vendor/larastan/larastan/extension.neon
  - ./larastan-baseline.neon

parameters:
  level: 7
  paths:
    - app
  excludePaths:
    - app/Console
    - app/Exceptions
    - app/Http/Middleware
    - app/Http/Kernel.php
    - app/Providers
    - app/Http/Controllers/Auth
  tmpDir: storage/phpstan
  reportUnmatchedIgnoredErrors: false
